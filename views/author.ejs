    <title>Portfolio</title>
  </head>

  <body>
    <div class="frame">
      <div class="main_frame">
        <div class="left_frame">
          <div class="left_1">
            <div class="left_1_1">
              <img
                src="images/Profile.jpeg"
                width="156px"
                height="142px"
                alt=""
              />
            </div>
            <div class="left_1_2"><%=user.name%></div>
            <div class="left_1_3"><%=user.profession%></div>
            <div class="left_icons">
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Instagram.svg" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Twitter.svg" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Youtube.svg" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Linkeding.svg" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Dribbble.svg" />
                </div>
              </a>
              <a class="link_href" href="">
                <div class="icon">
                  <img src="icon/Facebook.svg" />
                </div>
              </a>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left_2">
            <div class="left_2_1_box">
              <span>Age</span>
              <a><%=user.age%></a>
            </div>
            <div class="left_2_1_box">
              <span>residence</span>
              <a><%=user.residence%></a>
            </div>
            <div class="left_2_1_box">
              <span>Freelance</span>
              <a>unavailabe</a>
            </div>
            <div class="left_2_1_box">
              <span>Address</span>
              <a><%=user.address%></a>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left_3">
            <div class="left_3_1_box">
              <strong>Languages</strong>
              <span></span>
            </div>
            <div class="left_3_2_box">
              <span><%=user.language[0]%></span>
              <span>100%</span>
            </div>
            <div class="left_3_2_box">
              <span><%=user.language[1]%></span>
              <span>80%</span>
            </div>
            <div class="left_3_2_box">
              <span><%=user.language[2]%></span>
              <span>60%</span>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left_4">
            <div class="left_4_1_box">
              <strong>Skills</strong>
              <span></span>
            </div>
            <div class="left_4_1_box">
              <span><%=user.skills[0]%></span>
              <span>90%</span>
            </div>
            <div class="left_4_1_box">
              <span><%=user.skills[1]%></span>
              <span>85%</span>
            </div>
            <div class="left_4_1_box">
              <span><%=user.skills[2]%></span>
              <span>80%</span>
            </div>
            <div class="left_4_1_box">
              <span><%=user.skills[3]%></span>
              <span>75%</span>
            </div>
            <div class="left_4_1_box">
              <span><%=user.skills[4]%></span>
              <span>85%</span>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left_5">
            <strong>Extra Skills</strong>
            <div class="icon">
              <img src="icon/Icons.png" width="15px" height="15px" />
              <span><%=user.extra_skills[0]%></span>
            </div>
            <div class="icon">
              <img src="icon/Icons.png" width="15px" height="15px" />
              <span><%=user.extra_skills[1]%></span>
            </div>
            <div class="icon">
              <img src="icon/Icons.png" width="15px" height="15px" />
              <span><%=user.extra_skills[2]%></span>
            </div>
            <div class="icon">
              <img src="icon/Icons.png" width="15px" height="15px" />
              <span><%=user.extra_skills[3]%></span>
            </div>
          </div>
          <div class="marginer"></div>
        </div>
        <div class="midd_frame">
          <div class="top_container">
            <div class="header_container">
              <span><%=user.work_place%> </span>
              <p>
                Lorem ipsum dolor sit amet consectetur, adipisicing elit.
